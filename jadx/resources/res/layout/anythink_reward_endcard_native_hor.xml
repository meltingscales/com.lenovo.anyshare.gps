<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" android:background="@color/anythink_reward_endcard_hor_bg" android:layout_width="match_parent" android:layout_height="match_parent">
    <RelativeLayout android:id="@+id/anythink_native_ec_layout" android:layout_width="match_parent" android:layout_height="match_parent">
        <View android:id="@+id/anythink_center_view" android:layout_width="1dp" android:layout_height="1dp" android:layout_centerInParent="true"/>
        <RelativeLayout android:layout_width="match_parent" android:layout_height="match_parent" android:layout_above="@+id/anythink_center_view">
            <ImageView android:id="@+id/anythink_iv_adbanner_bg" android:layout_width="match_parent" android:layout_height="match_parent" android:scaleType="fitXY"/>
            <com.anythink.expressad.videocommon.view.RoundImageView android:id="@+id/anythink_iv_adbanner" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_margin="20dp" android:layout_centerInParent="true"/>
            <com.anythink.expressad.video.dynview.widget.AnyThinkDyImageView android:id="@+id/anythink_iv_flag" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_alignParentRight="true" android:layout_alignParentBottom="true"/>
        </RelativeLayout>
        <ImageView android:id="@+id/anythink_iv_iconbg" android:background="@drawable/anythink_reward_end_shape_oval" android:layout_width="64dp" android:layout_height="64dp" android:layout_centerInParent="true"/>
        <ImageView android:id="@+id/anythink_iv_icon" android:layout_width="48dp" android:layout_height="48dp" android:layout_centerInParent="true"/>
        <TextView android:textSize="15sp" android:textColor="@color/anythink_reward_title_textcolor" android:ellipsize="end" android:id="@+id/anythink_tv_apptitle" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="13dp" android:text="title" android:maxEms="7" android:singleLine="true" android:layout_below="@+id/anythink_iv_iconbg" android:layout_centerHorizontal="true"/>
        <com.anythink.expressad.videocommon.view.StarLevelView android:orientation="horizontal" android:id="@+id/anythink_sv_starlevel" android:layout_width="wrap_content" android:layout_height="20dp" android:layout_marginTop="2dp" android:layout_below="@+id/anythink_tv_apptitle" android:layout_centerHorizontal="true"/>
        <LinearLayout android:orientation="horizontal" android:id="@+id/anythink_tv_number_layout" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_below="@+id/anythink_sv_starlevel" android:layout_centerHorizontal="true">
            <TextView android:textSize="12sp" android:textColor="@color/anythink_reward_desc_textcolor" android:ellipsize="end" android:layout_gravity="center_vertical" android:layout_width="wrap_content" android:layout_height="match_parent" android:text="(" android:singleLine="true"/>
            <TextView android:textSize="12sp" android:textColor="@color/anythink_reward_desc_textcolor" android:ellipsize="end" android:layout_gravity="center_vertical" android:id="@+id/anythink_tv_number" android:layout_width="wrap_content" android:layout_height="match_parent" android:text="12345)" android:singleLine="true" android:drawableLeft="@drawable/anythink_reward_user" android:drawableStart="@drawable/anythink_reward_user"/>
        </LinearLayout>
        <TextView android:textSize="12sp" android:textColor="@color/anythink_reward_desc_textcolor" android:ellipsize="end" android:id="@+id/anythink_tv_appdesc" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="30dp" android:layout_marginTop="4dp" android:layout_marginRight="30dp" android:text="des" android:minLines="3" android:layout_below="@+id/anythink_tv_number_layout" android:layout_centerHorizontal="true" android:lineSpacingMultiplier="1.2"/>
        <ImageView android:id="@+id/anythink_iv_logo" android:layout_width="54dp" android:layout_height="10dp" android:layout_marginBottom="50dp" android:scaleType="fitXY" android:layout_alignParentBottom="true" android:layout_centerHorizontal="true"/>
        <TextView android:textSize="24sp" android:textColor="@android:color/white" android:gravity="center" android:id="@+id/anythink_tv_cta" android:background="@drawable/anythink_reward_shape_end_pager" android:layout_width="match_parent" android:layout_height="50dp" android:layout_marginLeft="24dp" android:layout_marginRight="24dp" android:layout_marginBottom="24dp" android:text="Install Now" android:layout_above="@+id/anythink_iv_logo"/>
        <RelativeLayout android:gravity="center" android:layout_gravity="center" android:id="@+id/anythink_native_ec_controller" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_margin="9dp">
            <ImageView android:id="@+id/anythink_iv_link" android:layout_width="35dp" android:layout_height="35dp" android:src="@drawable/anythink_reward_notice" android:layout_alignParentLeft="true" android:contentDescription="noteLinkButton"/>
            <com.anythink.expressad.widget.FeedBackButton android:gravity="center" android:id="@+id/anythink_native_endcard_feed_btn" android:paddingLeft="10dp" android:paddingRight="10dp" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="27dp" android:layout_marginLeft="9dp" android:layout_marginTop="4dp" android:text="@string/anythink_cm_feedback_btn_text" android:layout_toRightOf="@+id/anythink_iv_link" android:layout_alignTop="@+id/anythink_iv_link"/>
            <ImageView android:id="@+id/anythink_iv_close" android:layout_width="35dp" android:layout_height="35dp" android:src="@drawable/anythink_reward_close_ec" android:layout_alignParentRight="true" android:contentDescription="closeButton"/>
        </RelativeLayout>
    </RelativeLayout>
</RelativeLayout>
