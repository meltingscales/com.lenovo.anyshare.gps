<?xml version="1.0" encoding="utf-8"?>
<com.anythink.expressad.video.dynview.widget.AnyThinkRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" android:background="@color/anythink_reward_endcard_hor_bg" android:layout_width="match_parent" android:layout_height="match_parent">
    <RelativeLayout android:id="@+id/anythink_native_ec_layout" android:layout_width="match_parent" android:layout_height="match_parent">
        <RelativeLayout android:layout_width="match_parent" android:layout_height="0dp" android:layout_above="@+id/anythink_reward_end_card_offer_title_rl" android:layout_alignParentTop="true">
            <com.anythink.expressad.video.dynview.widget.AnyThinkDyImageView android:id="@+id/anythink_iv_adbanner_bg" android:layout_width="match_parent" android:layout_height="match_parent" android:scaleType="fitXY"/>
            <com.anythink.expressad.videocommon.view.RoundImageView android:id="@+id/anythink_iv_adbanner" android:layout_width="320dp" android:layout_height="180dp" android:layout_margin="10sp" android:layout_centerInParent="true"/>
            <com.anythink.expressad.video.dynview.widget.AnyThinkDyImageView android:id="@+id/anythink_iv_flag" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_alignParentRight="true" android:layout_alignParentBottom="true"/>
        </RelativeLayout>
        <RelativeLayout android:id="@+id/anythink_reward_end_card_offer_title_rl" android:layout_width="match_parent" android:layout_height="80dp" android:layout_alignParentBottom="true">
            <com.anythink.expressad.videocommon.view.RoundImageView android:id="@+id/anythink_iv_icon" android:layout_width="60dp" android:layout_height="60dp" android:layout_marginLeft="20dp" android:layout_marginRight="10dp" android:scaleType="centerCrop" android:layout_centerVertical="true"/>
            <TextView android:textSize="20sp" android:textStyle="bold" android:textColor="@android:color/black" android:ellipsize="end" android:id="@+id/anythink_tv_apptitle" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_marginTop="5dp" android:maxLines="1" android:layout_toLeftOf="@+id/anythink_tv_cta" android:layout_toRightOf="@+id/anythink_iv_icon" android:layout_alignTop="@+id/anythink_iv_icon"/>
            <com.anythink.expressad.video.dynview.widget.AnyThinkLevelLayoutView android:gravity="center_vertical" android:id="@+id/anythink_sv_starlevel" android:layout_width="0dp" android:layout_height="match_parent" android:layout_toLeftOf="@+id/anythink_tv_cta" android:layout_toRightOf="@+id/anythink_iv_icon" android:layout_below="@+id/anythink_tv_apptitle"/>
            <TextView android:textSize="18sp" android:textColor="@android:color/white" android:gravity="center" android:id="@+id/anythink_tv_cta" android:background="@drawable/anythink_reward_shape_end_pager" android:layout_width="180dp" android:layout_height="50dp" android:layout_marginRight="35dp" android:text="Install Now" android:layout_above="@+id/anythink_iv_logo" android:layout_alignTop="@+id/anythink_tv_apptitle" android:layout_alignParentRight="true"/>
            <com.anythink.expressad.video.dynview.widget.AnyThinkDyImageView android:id="@+id/anythink_iv_logo" android:layout_width="40dp" android:layout_height="10dp" android:layout_margin="10dp" android:layout_alignRight="@+id/anythink_tv_cta" android:layout_alignParentBottom="true"/>
        </RelativeLayout>
        <com.anythink.expressad.video.dynview.widget.AnythinkBaitClickView android:id="@+id/anythink_animation_click_view" android:visibility="gone" android:layout_width="match_parent" android:layout_height="match_parent"/>
    </RelativeLayout>
    <RelativeLayout android:id="@+id/anythink_native_ec_layer_layout" android:layout_width="match_parent" android:layout_height="match_parent"/>
    <RelativeLayout android:id="@+id/anythink_native_ec_controller" android:layout_width="match_parent" android:layout_height="match_parent">
        <com.anythink.expressad.video.dynview.widget.AnyThinkDyImageView android:id="@+id/anythink_iv_link" android:layout_width="25dp" android:layout_height="25dp" android:layout_margin="10dp" android:src="@drawable/anythink_reward_notice"/>
        <com.anythink.expressad.widget.FeedBackButton android:textSize="11sp" android:textColor="@android:color/white" android:gravity="center" android:id="@+id/anythink_native_endcard_feed_btn" android:paddingLeft="10dp" android:paddingRight="10dp" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="25dp" android:text="@string/anythink_cm_feedback_btn_text" android:layout_toRightOf="@+id/anythink_iv_link" android:layout_alignTop="@+id/anythink_iv_link"/>
        <com.anythink.expressad.video.dynview.widget.AnyThinkDyImageView android:id="@+id/anythink_iv_close" android:layout_width="25dp" android:layout_height="25dp" android:layout_margin="10dp" android:src="@drawable/anythink_reward_close" android:layout_alignParentRight="true" android:contentDescription="closeButton"/>
    </RelativeLayout>
</com.anythink.expressad.video.dynview.widget.AnyThinkRelativeLayout>
