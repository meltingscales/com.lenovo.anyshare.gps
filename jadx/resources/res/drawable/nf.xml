<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="true">
        <shape>
            <corners android:radius="@dimen/ai1"/>
            <solid android:color="@color/fz"/>
        </shape>
    </item>
    <item android:state_focused="true">
        <shape>
            <corners android:radius="@dimen/ai1"/>
            <solid android:color="@color/gy"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <corners android:radius="@dimen/ai1"/>
            <solid android:color="@color/fz"/>
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape>
            <corners android:radius="@dimen/ai1"/>
            <solid android:color="@color/ga"/>
        </shape>
    </item>
    <item>
        <shape>
            <corners android:radius="@dimen/ai1"/>
            <solid android:color="@color/fy"/>
        </shape>
    </item>
</selector>
