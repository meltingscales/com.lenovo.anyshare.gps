<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5dp"/>
            <gradient android:startColor="#ffd8d8d8" android:endColor="#ffd8d8d8" android:angle="0" android:centerY="0.5" android:centerColor="#ffd8d8d8"/>
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="5dp"/>
                <gradient android:startColor="#ffa2a2a2" android:endColor="#ffa2a2a2" android:angle="0" android:centerY="0.5"/>
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="5dp"/>
                <gradient android:startColor="@color/v4" android:endColor="@color/v4" android:angle="0" android:centerY="0.5"/>
            </shape>
        </clip>
    </item>
</layer-list>
