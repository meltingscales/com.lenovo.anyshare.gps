<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:gravity="center_vertical" android:id="@android:id/background" android:height="@dimen/br9">
        <shape>
            <corners android:radius="5dp"/>
            <solid android:color="#f1f1f1"/>
        </shape>
    </item>
    <item android:gravity="center_vertical" android:id="@android:id/secondaryProgress" android:height="@dimen/br9">
        <clip>
            <shape>
                <corners android:radius="5dp"/>
                <solid android:color="#f1f1f1"/>
            </shape>
        </clip>
    </item>
    <item android:gravity="center_vertical" android:id="@android:id/progress" android:height="@dimen/br9">
        <clip>
            <shape>
                <corners android:radius="5dp"/>
                <solid android:color="@color/v4"/>
            </shape>
        </clip>
    </item>
</layer-list>
