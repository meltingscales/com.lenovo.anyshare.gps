<?xml version="1.0" encoding="utf-8"?>
<com.anythink.expressad.video.dynview.widget.AnyThinkFramLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" android:background="@color/anythink_reward_endcard_hor_bg" android:layout_width="match_parent" android:layout_height="match_parent">
    <RelativeLayout android:id="@+id/anythink_native_ec_layout" android:layout_width="match_parent" android:layout_height="match_parent">
        <RelativeLayout android:layout_width="match_parent" android:layout_height="0dp" android:layout_marginBottom="35dp" android:layout_alignBottom="@+id/anythink_iv_icon" android:layout_alignParentTop="true">
            <com.anythink.expressad.video.dynview.widget.AnyThinkDyImageView android:id="@+id/anythink_iv_adbanner_bg" android:layout_width="match_parent" android:layout_height="match_parent" android:scaleType="fitXY"/>
            <com.anythink.expressad.videocommon.view.RoundImageView android:id="@+id/anythink_iv_adbanner" android:layout_width="match_parent" android:layout_height="200dp" android:layout_marginLeft="2dp" android:layout_marginRight="2dp" android:layout_centerInParent="true"/>
            <com.anythink.expressad.video.dynview.widget.AnyThinkDyImageView android:id="@+id/anythink_iv_flag" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_alignParentRight="true" android:layout_alignParentBottom="true"/>
        </RelativeLayout>
        <com.anythink.expressad.video.dynview.widget.AnyThinkDyImageView android:id="@+id/anythink_iv_icon" android:layout_width="70dp" android:layout_height="70dp" android:scaleType="centerCrop" android:layout_centerInParent="true" android:radius="10dp"/>
        <TextView android:textSize="22sp" android:textStyle="bold" android:textColor="@android:color/black" android:ellipsize="end" android:gravity="center_horizontal" android:id="@+id/anythink_tv_apptitle" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="10dp" android:maxLines="1" android:layout_below="@+id/anythink_iv_icon"/>
        <com.anythink.expressad.video.dynview.widget.AnyThinkLevelLayoutView android:gravity="center_horizontal" android:id="@+id/anythink_sv_starlevel" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="10dp" android:layout_below="@+id/anythink_tv_apptitle"/>
        <TextView android:textSize="24sp" android:textColor="@android:color/white" android:gravity="center" android:id="@+id/anythink_tv_cta" android:background="@drawable/anythink_reward_shape_end_pager" android:layout_width="match_parent" android:layout_height="50dp" android:layout_marginLeft="24dp" android:layout_marginRight="24dp" android:layout_marginBottom="24dp" android:text="Install Now" android:layout_above="@+id/anythink_iv_logo" android:layout_centerHorizontal="true"/>
        <com.anythink.expressad.video.dynview.widget.AnyThinkDyImageView android:id="@+id/anythink_iv_logo" android:layout_width="50dp" android:layout_height="18dp" android:layout_margin="10dp" android:layout_alignParentBottom="true" android:layout_centerHorizontal="true"/>
        <com.anythink.expressad.video.dynview.widget.AnythinkBaitClickView android:id="@+id/anythink_animation_click_view" android:visibility="gone" android:layout_width="match_parent" android:layout_height="match_parent"/>
    </RelativeLayout>
    <RelativeLayout android:id="@+id/anythink_native_ec_layer_layout" android:layout_width="match_parent" android:layout_height="match_parent"/>
    <RelativeLayout android:id="@+id/anythink_native_ec_controller" android:layout_width="match_parent" android:layout_height="match_parent">
        <com.anythink.expressad.video.dynview.widget.AnyThinkDyImageView android:id="@+id/anythink_iv_link" android:layout_width="25dp" android:layout_height="25dp" android:layout_marginLeft="10dp" android:layout_marginTop="10dp" android:layout_marginRight="10dp" android:layout_marginBottom="10dp" android:src="@drawable/anythink_reward_notice" android:layout_marginStart="10dp" android:layout_marginEnd="10dp"/>
        <com.anythink.expressad.widget.FeedBackButton android:textSize="11sp" android:textColor="@android:color/white" android:gravity="center" android:id="@+id/anythink_native_endcard_feed_btn" android:paddingLeft="10dp" android:paddingRight="10dp" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="25dp" android:text="@string/anythink_cm_feedback_btn_text" android:layout_toRightOf="@+id/anythink_iv_link" android:layout_alignTop="@+id/anythink_iv_link"/>
        <com.anythink.expressad.video.dynview.widget.AnyThinkDyImageView android:id="@+id/anythink_iv_close" android:layout_width="25dp" android:layout_height="25dp" android:layout_marginLeft="10dp" android:layout_marginTop="10dp" android:layout_marginRight="10dp" android:layout_marginBottom="10dp" android:src="@drawable/anythink_reward_close" android:layout_alignParentRight="true" android:contentDescription="closeButton" android:layout_marginStart="10dp" android:layout_marginEnd="10dp"/>
    </RelativeLayout>
</com.anythink.expressad.video.dynview.widget.AnyThinkFramLayout>
