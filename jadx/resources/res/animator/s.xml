<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="true" android:state_hovered="true">
        <objectAnimator android:duration="@integer/t" android:valueTo="@dimen/cb3" android:valueType="floatType" android:propertyName="translationZ"/>
    </item>
    <item android:state_enabled="true">
        <objectAnimator android:duration="@integer/t" android:valueTo="0" android:valueType="floatType" android:propertyName="translationZ"/>
    </item>
    <item>
        <objectAnimator android:duration="0" android:valueTo="@dimen/cb0" android:valueType="floatType" android:propertyName="translationZ"/>
    </item>
</selector>
