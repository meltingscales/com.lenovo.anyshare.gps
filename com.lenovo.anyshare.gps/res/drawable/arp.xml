<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="true">
        <shape>
            <corners android:radius="@dimen/br9" />
            <solid android:color="@color/wl" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <corners android:radius="@dimen/br9" />
            <solid android:color="@color/wl" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape>
            <corners android:radius="@dimen/br9" />
            <solid android:color="#4c247fff" />
        </shape>
    </item>
    <item>
        <shape>
            <corners android:radius="@dimen/br9" />
            <solid android:color="@color/v4" />
        </shape>
    </item>
</selector>