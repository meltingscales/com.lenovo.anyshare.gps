<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape>
            <corners android:radius="@dimen/bki" />
            <solid android:color="@color/apm" />
            <stroke android:width="@dimen/bkj" android:color="@color/apn" />
        </shape>
    </item>
    <item android:state_selected="true">
        <shape>
            <corners android:radius="@dimen/bki" />
            <solid android:color="@color/apm" />
            <stroke android:width="@dimen/bkj" android:color="@color/apn" />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape>
            <corners android:radius="@dimen/bki" />
            <solid android:color="@color/apm" />
            <stroke android:width="@dimen/bkj" android:color="@color/apn" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <corners android:radius="@dimen/bki" />
            <solid android:color="@color/apm" />
            <stroke android:width="@dimen/bkj" android:color="@color/apn" />
        </shape>
    </item>
    <item>
        <shape>
            <corners android:radius="@dimen/bki" />
            <solid android:color="@android:color/transparent" />
            <stroke android:width="@dimen/bkj" android:color="@color/w4" />
        </shape>
    </item>
</selector>