<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="@dimen/blv" />
            <stroke android:width="@dimen/bnc" android:color="@color/wh" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="@dimen/blv" />
                <solid android:color="@color/bi1" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="@dimen/blv" />
                <solid android:color="@color/wh" />
            </shape>
        </clip>
    </item>
</layer-list>