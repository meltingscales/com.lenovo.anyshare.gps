<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <corners android:radius="@dimen/cxs" />
            <solid android:color="#66ffb11b" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape>
            <corners android:radius="@dimen/cxs" />
            <solid android:color="#11ffb11b" />
        </shape>
    </item>
    <item>
        <shape>
            <corners android:radius="@dimen/cxs" />
            <solid android:color="#ffffb11b" />
        </shape>
    </item>
</selector>