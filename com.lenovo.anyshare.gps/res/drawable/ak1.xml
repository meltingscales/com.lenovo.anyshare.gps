<?xml version="1.0" encoding="utf-8"?>
<selector android:exitFadeDuration="@android:integer/config_mediumAnimTime"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape>
            <solid android:color="@color/wh" />
            <corners android:radius="@dimen/bqj" />
            <stroke android:width="0.0dip" android:color="@color/wh" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <solid android:color="#90247fff" />
            <corners android:radius="@dimen/bqj" />
            <stroke android:width="0.0dip" android:color="#90247fff" />
        </shape>
    </item>
</selector>