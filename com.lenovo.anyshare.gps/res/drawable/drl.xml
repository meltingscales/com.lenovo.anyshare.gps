<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="0.0dip" />
            <gradient android:startColor="#22ffffff" android:endColor="#22ffffff" android:angle="0.0" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip android:gravity="bottom" android:clipOrientation="vertical">
            <shape>
                <corners android:radius="0.0dip" />
                <gradient android:startColor="#ffddffff" android:endColor="#ddffffff" android:angle="0.0" />
            </shape>
        </clip>
    </item>
</layer-list>