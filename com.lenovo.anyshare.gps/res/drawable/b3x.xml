<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="true">
        <shape android:shape="rectangle">
            <solid android:color="#ffe9f2ff" />
            <corners android:radius="@dimen/c35" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <stroke android:width="1.0dip" android:color="#ffdcdcdc" />
            <corners android:radius="@dimen/c35" />
        </shape>
    </item>
</selector>