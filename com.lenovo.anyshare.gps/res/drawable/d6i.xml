<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="true">
        <shape>
            <corners android:radius="@dimen/dgb" />
            <solid android:color="@color/v5" />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape>
            <corners android:radius="@dimen/dgb" />
            <solid android:color="@color/b2r" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <corners android:radius="@dimen/dgb" />
            <solid android:color="@color/v5" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape>
            <corners android:radius="@dimen/dgb" />
            <solid android:color="@color/b39" />
        </shape>
    </item>
    <item>
        <shape>
            <corners android:radius="@dimen/dgb" />
            <solid android:color="@color/v4" />
        </shape>
    </item>
</selector>